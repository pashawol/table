    "postcss-nested": "^6.0.1",
    "postcss-scss": "^4.0.9",
    "postcss-sort-media-queries": "^5.2.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^3.1.1",
    "prettier-eslint": "^16.2.0",
