    "colors": "^1.4.0",
    "cssnano": "^6.0.2",
    "del": "^7.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "gulp": "^4.0.2",
    "gulp-cheerio": "^1.0.0",
    "gulp-data": "^1.3.1",
    "gulp-notify": "^4.0.0",
    "gulp-npm-dist": "^1.0.4",
    "gulp-plumber": "^1.2.1",
